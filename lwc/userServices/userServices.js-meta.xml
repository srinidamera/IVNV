<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>61.0</apiVersion>
	<isExposed>true</isExposed>
	<masterLabel>User Services</masterLabel>
	<targets>
		<target>lightningCommunity__Page</target>
		<target>lightningCommunity__Default</target>
	</targets>
	<targetConfigs>
		<targetConfig targets="lightningCommunity__Default">
			<property
					type="Integer"
					name="tileHeight"
					label="Height for cards in pixels"
					default="104"
			/>
			<property
					type="String"
					name="labelHyperlink"
					label="Label for details hyperlink"
					default="View Details"
					translatable="true"
			/>

			<property
					type="String"
					name="labelLastUpdated"
					label="Label for 'Last Updated'"
					default="Last Updated"
					translatable="true"
			/>

			<property
					type="String"
					name="labelStatusNew"
					label="Label for status 'Open'."
					default="Open"
					translatable="true"
			/>
			<property
					type="String"
					name="labelStatusOnHold"
					label="Label for status 'On Hold'."
					default="On Hold"
					translatable="true"
			/>
			<property
					type="String"
					name="labelStatusConverted"
					label="Label for status 'Converted'."
					default="Completed"
					translatable="true"
			/>
			<property
					type="String"
					name="labelStatusClosedIncomplete"
					label="Label for status 'Closed - Incomplete'."
					default="Incomplete"
					translatable="true"
			/>
		</targetConfig>
	</targetConfigs>
</LightningComponentBundle>